<!--jd.js实现轮播广告-->
/**
 * Created by Administrator on 2016/6/29.
 */
window.onload=function(){
    var btn1=document.getElementById("btn1");
    var btn2=document.getElementById("btn2");
    var pic_list_li=document.getElementsByClassName("pic_list_li")[0];
    var picWidth=parseInt(document.querySelector("#slider  .pic_list_li img").width);
    var btn_list=document.querySelectorAll(" #slider .slider_adv ul li a");
    var pic_list=document.getElementsByClassName("pic_list")[0];
    console.log(pic_list);
    var index=0;
    btn_list[index].style.color="red";
    btn2.onclick=function() {
        index++;
        for (var i = 0; i < btn_list.length; i++) {
            if (index == 5) {
                index = 0
            }
            pic_list_li.style.left = (-picWidth) * index + "px";
            if(index==i){btn_list[i].style.color="red" }
            else{btn_list[i].style.color=""}
        }
    };
    btn1.onclick=function(){
        index--;
        if(index<0){index=4}
        pic_list_li.style.left=(-picWidth)*index+"px";
        for (var i = 0; i < btn_list.length; i++) {
            if(index==i){btn_list[i].style.color="red" }
            else{btn_list[i].style.color=""}
        }
    };
    var time=setInterval(function(){btn2.onclick()},1500);
    pic_list.onmouseover=function(){clearInterval(time)};
    pic_list.onmouseout=function(){ time=setInterval(function(){btn2.onclick()},1500)};

}
